# This loads the records that apply to the entire BNC-505.
file "$(DELAYGEN)/delayGenApp/Db/BNC_505.db"
{
pattern
{P,            R,         PORT    }
{13Laser:,     BNC1:,     serial4 }
}

# This loads the records for each BNC-505 output.
file "$(DELAYGEN)/delayGenApp/Db/BNC_505_Pn.db"
{
pattern
{P,            R,        PORT,      N}
{13Laser:,     BNC1:,    serial4,   1}
{13Laser:,     BNC1:,    serial4,   2}
}
