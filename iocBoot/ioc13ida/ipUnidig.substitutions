# Set the SCAN field of the longin record and bi record #0 to "1 second", rather than I/O
# interrupt, because the quadEM toggles input #0 at 800Hz, and that is too much load on the IOC

file "$(IPUNIDIG)/ipUnidigApp/Db/IpUnidigLi.db"
{
pattern
{P,       R,          PORT,       SCAN}
{13IDA:,  Unidig1Li,  Unidig1, "1 second"}
}

file "$(IPUNIDIG)/ipUnidigApp/Db/IpUnidigBi.db"
{
pattern
{P,       R,          PORT,    MASK,        SCAN}
{13IDA:, Unidig1Bi0,  Unidig1, 0x000001, "1 second"}
{13IDA:, Unidig1Bi1,  Unidig1, 0x000002, "I/O Intr"}
{13IDA:, Unidig1Bi2,  Unidig1, 0x000004, "I/O Intr"}
{13IDA:, Unidig1Bi3,  Unidig1, 0x000008, "I/O Intr"}
{13IDA:, Unidig1Bi4,  Unidig1, 0x000010, "I/O Intr"}
{13IDA:, Unidig1Bi5,  Unidig1, 0x000020, "I/O Intr"}
{13IDA:, Unidig1Bi6,  Unidig1, 0x000040, "I/O Intr"}
{13IDA:, Unidig1Bi7,  Unidig1, 0x000080, "I/O Intr"}
{13IDA:, Unidig1Bi8,  Unidig1, 0x000100, "I/O Intr"}
{13IDA:, Unidig1Bi9,  Unidig1, 0x000200, "I/O Intr"}
{13IDA:, Unidig1Bi10, Unidig1, 0x000400, "I/O Intr"}
{13IDA:, Unidig1Bi11, Unidig1, 0x000800, "I/O Intr"}
{13IDA:, Unidig1Bi12, Unidig1, 0x001000, "I/O Intr"}
{13IDA:, Unidig1Bi13, Unidig1, 0x002000, "I/O Intr"}
{13IDA:, Unidig1Bi14, Unidig1, 0x004000, "I/O Intr"}
{13IDA:, Unidig1Bi15, Unidig1, 0x008000, "I/O Intr"}
{13IDA:, Unidig1Bi16, Unidig1, 0x010000, "I/O Intr"}
{13IDA:, Unidig1Bi17, Unidig1, 0x020000, "I/O Intr"}
{13IDA:, Unidig1Bi18, Unidig1, 0x040000, "I/O Intr"}
{13IDA:, Unidig1Bi19, Unidig1, 0x080000, "I/O Intr"}
{13IDA:, Unidig1Bi20, Unidig1, 0x100000, "I/O Intr"}
{13IDA:, Unidig1Bi21, Unidig1, 0x200000, "I/O Intr"}
{13IDA:, Unidig1Bi22, Unidig1, 0x400000, "I/O Intr"}
{13IDA:, Unidig1Bi23, Unidig1, 0x800000, "I/O Intr"}
}

