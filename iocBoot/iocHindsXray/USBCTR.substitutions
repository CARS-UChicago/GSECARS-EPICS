{   HindsXray:USBCTR:,  Bi2,  0x02   USBCTR,     0}
{   HindsXray:USBCTR:,  Bi3,  0x04   USBCTR,     0}
{   HindsXray:USBCTR:,  Bi4,  0x08   USBCTR,     0}
{   HindsXray:USBCTR:,  Bi5,  0x10   USBCTR,     0}
{   HindsXray:USBCTR:,  Bi6,  0x20   USBCTR,     0}
{   HindsXray:USBCTR:,  Bi7,  0x40   USBCTR,     0}
{   HindsXray:USBCTR:,  Bi8,  0x80   USBCTR,     0}
