# Direction bits on binary I/O
#  VAL 0=input, 1=output
file "$(MEASCOMP)/db/measCompBinaryDir.template"
{
pattern
{    P,                 R,   MASK,  VAL,  PORT,    ADDR}
{   HindsXray:USBCTR:,  Bd1,  0x01    0,  USBCTR,     0}
{   HindsXray:USBCTR:,  Bd2,  0x02    0,  USBCTR,     0}
{   HindsXray:USBCTR:,  Bd3,  0x04    0,  USBCTR,     0}
{   HindsXray:USBCTR:,  Bd4,  0x08    0,  USBCTR,     0}
{   HindsXray:USBCTR:,  Bd5,  0x10    1,  USBCTR,     0}
{   HindsXray:USBCTR:,  Bd6,  0x20    1,  USBCTR,     0}
{   HindsXray:USBCTR:,  Bd7,  0x40    1,  USBCTR,     0}
{   HindsXray:USBCTR:,  Bd8,  0x80    1,  USBCTR,     0}
}

