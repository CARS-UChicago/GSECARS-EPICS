# This loads the records that apply to the entire BNC-505.
file "$(DELAYGEN)/db/BNC_505.db"
{
pattern
{P,            R,         PORT    }
{13BNC1:,     BNC1:,     serial1 }
}

# This loads the records for each BNC-505 output.
file "$(DELAYGEN)/db/BNC_505_Pn.db"
{
pattern
{P,            R,        PORT,      N}
{13BNC1:,     BNC1:,    serial1,   1}
{13BNC1:,     BNC1:,    serial1,   2}
}
